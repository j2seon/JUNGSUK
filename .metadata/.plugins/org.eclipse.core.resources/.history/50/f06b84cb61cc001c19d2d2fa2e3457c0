class St{
	String name;
	int ban;
	int no;
	int kor;
	int eng;
	int math;
	
	
	public St(String name, int ban, int no, int kor, int eng, int math) {
		super();
		this.name = name;
		this.ban = ban;
		this.no = no;
		this.kor = kor;
		this.eng = eng;
		this.math = math;
	}
	
	
	
	
}
