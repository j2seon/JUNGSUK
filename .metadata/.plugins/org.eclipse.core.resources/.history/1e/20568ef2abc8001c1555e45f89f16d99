package chap4;

public class Solution {

	 public int[] solution(int[] arr, int divisor) {
	        int[] answer = {};
	        int num=0;
	        for(int i=0; i<arr.length; i++) {
	            if(arr[i]%divisor!=0)
	            	answer[0]=-1;
	            
	            
	            if(arr[i]%divisor==0) {
	            	answer[num]+=arr[i];
	            	num++;
	            }
	            
	            
	        }
	        
	        
	        return answer;
	    }
	
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
