package chap7;

class MyPoint extends Object{
	int x; 
	int y;
//	String getLocation() {
//		return "x:"+x+", y:"+y;
//	}
	@Override
	//Object 클래스의 toString()을 오버라이딩
	public String toString() {
		return "x:"+x+", y:"+y;
	}
	
	
	
	
}

