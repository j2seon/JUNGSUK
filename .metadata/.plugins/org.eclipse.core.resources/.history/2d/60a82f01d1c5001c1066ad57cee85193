public class Ex7_5 {
	public static void main(String[] args) {
//		MyPoint3D p = new MyPoint3D();
//		p.x=3;
//		p.y=5;
//		p.z=7;
//		System.out.println(p.getLocation());
		
		MyPoint p = new MyPoint();
		p.x=3;
		p.y=5;
		System.out.println(p.toString()); // 위의 오버라이딩 한 값이 나온다.
		System.out.println(p);
	}

}
