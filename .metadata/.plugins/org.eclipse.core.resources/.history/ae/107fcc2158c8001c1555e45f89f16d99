package chap6;


class Student{
	String name ;
	int ban;
	int no;
	int kor;
	int eng;
	int math;
	
	int getTotal() {
		return kor+eng+math;
	}
	float getAgerage() {
		float sum=0;
		sum=(float)getTotal()/3;
		return sum;
	}
	
}


