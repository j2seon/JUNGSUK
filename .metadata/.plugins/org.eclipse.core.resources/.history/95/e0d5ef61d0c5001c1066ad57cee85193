package chap7;

class MyPoint{
	int x; 
	int y;
	String getLocation() {
		return "x:"+x+", y:"+y;
	}
	
}

class MyPoint3D extends MyPoint{
	int z;
	
	//조상의 getLocation을 오버라이딩한다.
	String getLocation() {
		return "x:"+x+", y:"+y+", z:"+z;
	}
}



public class Ex7_5 {
	public static void main(String[] args) {
		MyPoint3D p = new MyPoint3D();
		p.x=3;
		p.y=5;
		p.z=7;
		System.out.println(p.getLocation());
	}

}
