package chap6;

public class Ex6_6 {

	static double getDistance(int x, int y, int x1, int y1) {
		return Math.sqrt(((x-x1)*(x-x1))+((y-y1)*(y-y1)));
	}
	
	/*
	 * 두점사이의 공식 
	 * 
	 * (x-x1)제곱 +(y-y1)제곱 의 제곱근! 
	 * 
	 * */
	
	
	
	public static void main(String[] args) {
		System.out.println(getDistance(1,1,2,2));

	}

}
