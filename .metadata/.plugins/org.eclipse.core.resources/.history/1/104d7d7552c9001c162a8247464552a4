package chap7;

class Product{
	int price, bounsPoint;
	
	Product(int price){
		this.price=price;
		this.bounsPoint=(int)(price/10); //가격의 10%가 보너스 가격이다.
	}
}

class Tv1 extends Product{
	Tv1(){
		super(100);
		
	}
}
	




