package chap4;


class Solution{
	 public int[] solution(int[] arr, int divisor) {
	        int[] answer = {};
	        int num=0;
	        for(int i=0; i<arr.length; i++) {
	            if(arr[i]%divisor!=0)
	            	answer[0]=-1;
	            if(arr[i]%divisor==0) {
	            	answer[num]+=arr[i];
	            	num++;
	            }
	        }
	        return answer;
	    }
}


public class Solutionaaaa {
	public static void main(String[] args) {
		Solution s = new Solution();
		
		System.out.println(s.solution(new int[] {5,8,7,10}, 5));
				
	}

}
