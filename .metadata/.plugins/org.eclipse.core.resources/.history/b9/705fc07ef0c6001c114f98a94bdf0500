package chap7;


//생성사의 첫줄에 반드시 생성자를!! 

class MyPoint1{
	int x; 
	int y;
	
	MyPoint1() {}
	
	MyPoint1(int x, int y){
		this.x=x;
		this.y=y;
	}
	String getLocation() {
		return "x : "+x+", y : "+y;
	}
}
class Point3d{
	int z;
	
	Point3d(int x , int y ,int z) {
		this.x=x;
		this.y=y;
		this.z=z;
	}
	
}


