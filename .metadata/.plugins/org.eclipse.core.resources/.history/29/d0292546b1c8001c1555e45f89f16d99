package chap4;


class Solution{
	 public int[] solution(int[] arr, int divisor) {
	 
		 int[] answer= {};
		 for(int i=0; i<arr.length; i++) {
			 if(arr[i]%divisor!=0) {
				 answer = new int[] {-1};
				 return answer;
			 }
			 if(arr[i]%divisor==0) {
				 int num=0;
				 answer[num]=arr[i];
			 }
		 }
		return answer;
		 
	 
	 
	 }
	     
}


public class Solutionaaaa {
	public static void main(String[] args) {
		Solution s = new Solution();
		
		System.out.println(s.solution(new int[] {5,8,7,10}, 5));
				
	}

}
