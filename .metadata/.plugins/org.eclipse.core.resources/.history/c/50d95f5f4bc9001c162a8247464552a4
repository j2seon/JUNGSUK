package chap7;


class Car_1{
	String color;
	int door;
	
	void drive() {
		System.out.println("何釜何釜");
	}
	void stop() {
		System.out.println("stop!");
	} 
}

class FireEngine extends Car_1{
	void water() { //拱何府绰 扁瓷
		System.out.println("water!");
	}
} 

public class Chap7_15 {
	public static void main(String[] args) {
		
		Car_1 c = null;
		FireEngine f1 = new FireEngine();
		FireEngine f2=null;
		
		f1.water();
		
		
	}

}
