package chap7;

class Product{
	int price, bounsPoint;
	
	Product(int price){
		this.price=price;
		this.bounsPoint=(int)(price/10); //가격의 10%가 보너스 가격이다.
	}
}

class Tv11 extends Product{
	Tv11(){
		super(100); //Tv가격
	}

	public String toString() {
		return "Tv";
	}
	
}
	
class Computer extends Product{
	public Computer() {
		super(200); //가격
	}

	public String toString() {
		return "Computer";
	}
}

class Buyer{
	int money = 1000;
	int bounsPoint=0;
	
	
	
}



