package chap7;

class Product1{
	int price, bounsPoint;
	
	Product1(int price){
		this.price=price;
		this.bounsPoint=(int)(price/10); //가격의 10%가 보너스 가격이다.
	}
	public Product1() {}
}
class Tv12 extends Product{
	Tv12(){
		super(100); //Tv가격
	}

	public String toString() {
		return "Tv";
	}
	
}
	
class Computer1 extends Product{
	public Computer1() {
		super(200); //가격
	}

	public String toString() {
		return "Computer";
	}
}

class Gold1 extends Product{
	public Gold1() {
		super(1000);
	}
	public String toString() {
		return "gold";
	}
}


