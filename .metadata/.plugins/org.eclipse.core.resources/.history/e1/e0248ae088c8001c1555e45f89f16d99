package chap7;



class Point{
	int x, y;
	
	public Point() {
		this(0, 0);
	}
	public Point(int x, int y) {
		this.x = x;
		this.y = y;
	}
	String getXY() {
		return"("+x+","+y+")"; //x와 y를 문자열로
	}
}
class shape{//색 
	String color="black";
	void draw() {
		System.out.printf("[color=%s]%n",color);
	}
	
	
}



