class SutdaDeck{
	final int CARD_NUM=20;
	S
}

class SutdaCard{
	int num;
	boolean isKwang;
	
	
	public SutdaCard() {
		this(1, true);
	}


	public SutdaCard(int num, boolean isKwang) {
		this.num = num;
		this.isKwang = isKwang;
	}
	
}