package chap4;

//별찍기
public class Ex4_15 {

	public static void main(String[] args) {
		//바깥쪽 5번
		for(int i=0; i<=5; i++) {
			//안쪽 10번 줄바꿈 없이!!
			for(int j=0; j<=10; j++) {
				System.out.print("*");
			}			
			System.out.println();
		}
		System.out.println();
		
		
		for(int i=1; i<=5; i++) {
			for(int j=0;j<i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		
		
		
		
		
	}
}
