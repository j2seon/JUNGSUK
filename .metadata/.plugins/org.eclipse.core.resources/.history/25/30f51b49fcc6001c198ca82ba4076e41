package chap6;

public class Ex6_12 {
	public static void main(String[] args) {
		
		int [] arr= new int[] {3,2,1,6,5,4};
		printArr(arr);
		
		
		
	}
	static void printArr(int[] arr) {
		System.out.println("[");
		
		for(int i : arr) 
			System.out.println(i+",");
		System.out.println("]");
		
		
	}
	
	

}
